CREATE  TABLE IF NOT EXISTS `birthright`.`discharges` (
  `id` INT(11) NOT NULL AUTO_INCREMENT ,
  `name` VARCHAR(45) NULL DEFAULT NULL ,
  `commission_id` INT(11) NULL DEFAULT NULL ,
  `processNumber` VARCHAR(45) NULL DEFAULT NULL ,
  `owner_id` VARCHAR(45) NULL DEFAULT NULL ,
  `created` DATETIME NULL DEFAULT NULL ,
  `modified` DATETIME NULL DEFAULT NULL ,
  PRIMARY KEY (`id`) )
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8
COLLATE = utf8_general_ci, 
COMMENT = 'Tabela para descarga' ;

CREATE  TABLE IF NOT EXISTS `birthright`.`commissions` (
  `id` INT(11) NOT NULL AUTO_INCREMENT ,
  `name` VARCHAR(45) NULL DEFAULT NULL ,
  `user_id_1` INT(11) NULL DEFAULT NULL ,
  `user_id_2` INT(11) NULL DEFAULT NULL ,
  `user_id_3` INT(11) NULL DEFAULT NULL ,
  `user_id_4` INT(11) NULL DEFAULT NULL ,
  `user_id_5` INT(11) NULL DEFAULT NULL ,
  `user_id_6` INT(11) NULL DEFAULT NULL ,
  `created` DATETIME NULL DEFAULT NULL ,
  `modified` DATETIME NULL DEFAULT NULL ,
  PRIMARY KEY (`id`) )
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8
COLLATE = utf8_general_ci;

CREATE  TABLE IF NOT EXISTS `birthright`.`commissions_discharges` (
  `id` INT(11) NOT NULL AUTO_INCREMENT ,
  `commission_id` INT(11) NULL DEFAULT NULL ,
  `discharge_id` INT(11) NULL DEFAULT NULL ,
